<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="/faces/publico/erro.xhtml">
    <class-loader delegate="true"/>
    <security-role-mapping>
        <role-name>aluno</role-name>
        <group-name>aluno</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>servidor</role-name>
        <group-name>servidor</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>administrador</role-name>
        <group-name>administrador</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>servidor_chefe</role-name>
        <group-name>servidor_chefe</group-name>
    </security-role-mapping>
    <jsp-config>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class' java code.</description>
        </property>
    </jsp-config>
</glassfish-web-app>